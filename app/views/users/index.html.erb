  <div class="container px-5 px-sm-0">
  <div class="row">
    
  <%= render 'books/list', book: @book, books: @books %>
  
  <div class= "col-md-8 offset-md-1">
    <h2>Users</h2>
    <table class='table'>
    <tr>
    <th>Image</th>
    <th>Name</th>
    </tr>

    <% @users.each do |users| %>
    <tr>
   <th><%= image_tag users.get_profile_image(100,100) %></th>
   <th><%= users.name %><th>
   <th><%= link_to "Show",user_path(users.id) %></th>
   </tr>
   <% end %>
   </table>
  </div>
  </div>
  </div>
